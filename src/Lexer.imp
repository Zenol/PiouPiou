/* Lexer's implementation */

namespace PiouC
{
    template<>
    inline
    int
    Lexer::get_last_token_value() const
    { return last_token_int; }

    template<>
    inline
    std::string
    Lexer::get_last_token_value() const
    { return last_token_string; }

    template<>
    inline
    char
    Lexer::get_last_token_value() const
    { return last_token_char; }
}
